   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle x^{2} + 5 x y z + y^{3} + z^{4}$"
      ],
      "text/plain": [
       "x**2 + 5*x*y*z + y**3 + z**4"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "x, y, z = symbols('x,y,z', real=True)\n",
    "f = x**2 + y**3 + z**4 + 5*x*y*z\n",
    "f"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "299e5909-7d44-47bc-9dfd-c243914ac220",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " f_x= 2*x + 5*y*z\n",
      " f_y= 5*x*z + 3*y**2\n",
      " f_z= 5*x*y + 4*z**3\n"
     ]
    }
   ],
   "source": [
    "for var in [x,y,z]:\n",
    "    print(\" f_\" + str(var) + \"=\",f.diff(var))"
   ]
